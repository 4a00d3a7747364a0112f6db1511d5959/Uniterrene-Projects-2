/* CSS Document */

  

/* ************ Top Header ************** */

#top-header {
	background-color: #d9d7d7;
	width: 100%;
	height: auto;
}
#top-header .top-header_left ul li {
	display: inline-block;
	font-size: 20px;
	font-family: 'Karla-Regular';
	color: #000;
}
#top-header .top-header_left ul li img {
	margin-right: 5px;
}
#top-header .container {
	text-align: center;
}
#top-header .top-header_left {
	float: left;
	height: auto;
	padding: 13px 0 13px 0;
}
#top-header .top-header_middle {
	background-color: #ef3138;
	display: inline-block;
	height: auto;
	padding: 9px 13px;
}
#top-header .top-header_middle ul {
	text-align: center;
}
#top-header .top-header_middle ul li {
	display: inline-block;
	color: #fff;
	font-family: 'Karla-Bold';
	font-size: 16px;
	text-align: left;
}
#top-header .top-header_middle ul li span {
	display: block;
	font-size: 12px;
	font-family: 'Karla-Regular';
	line-height: 14px;
}
#top-header .top-header_middle ul li img {
	margin-right: 10px;
}
#top-header .top-header_right {
	float: right;
	height: auto;
	padding: 15px 0;
	text-align: right;
}
#top-header .top-header_right ul li {
	display: inline-block;
	margin-right: 5px;
}
#top-header .top-header_right ul li:last-child {
	margin-right: 0px;
}
#top-header .top-header_right ul li a {
	color: #000;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
#top-header .top-header_right ul li span {
	padding-left: 8px;
	font-size: 18px;
	font-family: 'Karla-Regular';
	color: #000;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
#top-header .top-header_right ul li i {
	font-size: 18px;
}
#middle-header {
	width: 100%;
	height: auto;
	background: #fff;
	padding: 15px 0 0;
}
#middle-header .container {
	text-align: center;
}
#middle-header .middle-header_left {
	height: auto;
	float: left;
	padding: 10px 0 20px;
}
#middle-header .middle-header_left li {
	font-family: Roboto-Medium;
	font-size: 16px;
	display: block;
	text-align: left;
	color: #606566;
}
.search button.btn {
	width: 38px;
	height: 38px;
	cursor: pointer;
	text-indent: -9999px;
	background: url(../images/search_icon.png) no-repeat center center;
	position: absolute;
	top: 4px;
	right: 6px;
}
#middle-header .middle-header_search {
	width: 30%;
	position: relative;
	display: inline-block;
	margin-top: 12px;
}
 .middle-header_search input, .middle-header_search input::-webkit-input-placeholder {
 font-size: 12px;
 color: #000;
 text-indent: 20px;
}
.middle-header_search input {
	height: 40px;
	width: 88%;
}
.middle-header_search {
	border: 1px solid #e5e4e4;
}
.middle-header_search input {
	border: none;
	float: left;
	text-indent: 11px;
}
.middle-header_cart {
	float: left;
	padding: 25px 40px 25px 0;
	position: relative;
}
.middle-header_cart ul li {
	display: inline-block;
	vertical-align: middle;
}
.middle-header_cart ul li .count span {
	font-size: 11px;
}
.middle-header-right {
	float: right;
}
.middle-header_cart ul li span {
	font-size: 15px;
}
.middle-header_cart ul li img {
	margin-right: 10px;
}
.middle-header_cart .count {
	width: 20px;
	height: 20px;
	background: #ef3138;
	border-radius: 50%;
	color: #fff;
	font-size: 12px;
	position: absolute;
	top: 13px;
	left: 16px;
}
.count span {
	display: block;
	text-align: center;
	line-height: 20px;
}
.middle-header_online ul li {
	font-family: Roboto-Medium;
	font-size: 13px;
	color: #606566;
	display: block;
	line-height: 14px;
}
.middle-header_online ul {
	padding-top: 4px;
	text-align: center;
}
.middle-header_online {
	float: right;
	text-align: center;
}
.navigation {
	background: #f9f9f9;
}
.nav_container {
	width: 77%;
	height: auto;
	float: left;
	position: relative;
}
.nav_container ul li {
	display: inline-block;
	margin: 0 10px;
}
.nav_container ul li a {
	text-transform: uppercase;
	font-size: 14px;
	color: #000;
	padding: 20px 15px;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
	position: relative;
	text-align: center;
	display: block;
}
.nav_container > .navBox > ul > li > a:hover {
	background: #ef3138;
	color: #fff;
}
.nav_container ul li i {
	margin-left: 5px;
	display: inline-block;
}
.nav_container ul li:first-child {
	margin: 0px;
}
.nav_container ul li .sub_menu {
	width: 100%;
	background: #fff;
	padding: 30px 0;
	position: absolute;
	z-index: 999999;
	top: 57px;
	left: 0;
	display: none;
}
.nav_container > .navBox > ul > li.active > a {
	background: #ef3138;
	color: #fff;
}
.nav_container ul li:hover .sub_menu {
	display: block;
}
.nav_container ul li a::before {
	background-image: url(../images/menu_arrow.png);
	background-repeat: no-repeat;
	content: '';
	width: 16px;
	height: 10px;
	position: absolute;
	left: 43%;
	bottom: -8px;
	display: none;
	z-index: 9999999;
}
.nav_container ul li:hover a:before {
	display: block;
}
.nav_container ul li ul.sub_menu li a:before {
	display: none !important;
}
.nav_container ul li a i {
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
.nav_container ul li:hover a i {
	-ms-transform: rotate(180deg); /* IE 9 */
	-webkit-transform: rotate(180deg); /* Chrome, Safari, Opera */
	transform: rotate(180deg);
}
.nav_container ul li ul.sub_menu li:after {
	background-image: url(../images/submenu_divider.png);
	content: '';
	width: 1px;
	height: 100%;
	position: absolute;
	right: 0;
	bottom: 0;
	display: block;
	z-index: 99999999999;
}
.nav_container ul li ul.sub_menu li:last-child:after {
	background-image: none;
}
.sub_menu li a span {
	display: block;
	float: left;
	margin-right: 8px;
}
.nav_container ul li ul.sub_menu li a {
	color: #222;
	text-transform: capitalize;
	font-family: 'Karla-Bold';
	font-size: 15px;
	vertical-align: top;
	padding: 0 5px 0 0px;
	text-align: left;
	transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-ms-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	-webkit-transition: all 0.5s ease-in-out;
}
.nav_container ul li ul.sub_menu li a:hover{
	color: #ef3138;
}
.nav_container .sub_menu li:first-child a {
	padding-left: 0;
	display: block;
}
.nav_container .sub_menu li a:hover {
	color: #ec343e;
}
.nav_container ul li ul.sub_menu li {
	position: relative;
	width: 19%;
	vertical-align: top;
	margin-left: 35px;
	margin-bottom: 20px;
}
/*.nav_container ul li ul.sub_menu li:first-child {

	margin-left: 0px;

}*/



.nav_container ul li ul.sub_menu li {
	margin-right: 0px;
}
.categories_menu {
	width: 23%;
	float: right;
	background: #232323;
	position: relative;
}
.categories_menu ul li a {
	color: #fff;
	font-size: 16px;
	padding: 18px 26px;
	display: block;
}
.categories_menu ul li i {
	font-size: 20px;
	margin-right: 20px;
}
.categories_menu ul li ul li {
	border-bottom: 1px solid #bab9b9;
}
.categories_menu ul li ul li:last-child {
	border-bottom: hidden;
}
.categories_menu ul li ul li:first-child {
	border-top: 1px solid #bab9b9;
}
.categories_menu ul li ul {
	display: none;
	position: absolute;
	z-index: 99999;
	background: #232323;
	width: 100%;
}
/* ************ Banner ************** */



